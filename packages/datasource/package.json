{
  "name": "@orpheus/datasource-provider",
  "version": "1.0.0",
  "description": "Orpheus datasource provider package",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:alias": "tsc-alias -p tsconfig.json",
    "clean": "rm -rf dist",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:all": "yarn clean && yarn build && yarn build:alias && rollup -c rollup.config.mjs && rm -rf ./dist/src/*.d.ts"
  },
  "author": "Soumil Singh",
  "license": "ISC",
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "rollup": "^4.43.0",
    "rollup-plugin-dts": "^6.2.1"
  },
  "dependencies": {
    "got": "^14.4.7",
    "nats": "^2.29.3",
    "pg": "^8.16.0",
    "redis": "^5.5.6",
    "winston": "^3.17.0"
  }
}
