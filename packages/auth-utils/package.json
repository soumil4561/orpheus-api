{
  "name": "@orpheus/auth-utils",
  "version": "1.0.0",
  "description": "Orpheus Auth utils",
  "main": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:alias": "tsc-alias -p tsconfig.json",
    "clean": "rm -rf dist",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:all": "yarn clean && yarn build && yarn build:alias && rollup -c rollup.config.mjs && rm -rf ./dist/src/*.d.ts"
  },
  "author": "Soumil Singh",
  "license": "ISC",
  "devDependencies": {
    "rollup": "^4.43.0",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2"
  }
}
